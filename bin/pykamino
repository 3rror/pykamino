#!/usr/bin/env python

import argparse

parser = argparse.ArgumentParser()
subparsers = parser.add_subparsers(help='Functions')
parser_scraper = subparsers.add_parser('scraper', help='Fetch data from various services')
parser_features = subparsers.add_parser('features', help='Calculate features from the data downloaded')

parser.parse_args()
